                         tblReceivMain.WheelsChocked, tblReceivMain.FinishTime, tblReceivMain.StartTime, Employees_1.LastName + ', ' + Employees_1.FirstName AS UnloadedBy, Employees.LastName + ', ' + Employees.FirstName AS CheckedBy, 
                         ShipmentConditions.ShipmentCondition AS LoadCondition, ShipmentConditions_1.ShipmentCondition AS PhysicalCondition, Vendor.VendorName, SUM(tblReceivDet.intUnits) AS intUnits
FROM            tblReceivMain INNER JOIN
                         tblReceivDet ON tblReceivMain.ReceivID = tblReceivDet.ReceivMainID INNER JOIN
                         tblCustomers ON tblReceivMain.ReceivCustID = tblCustomers.CustomerID INNER JOIN
                         tblCarriers ON tblReceivMain.ReceivCarrierID = tblCarriers.CarrierID INNER JOIN
                         tblItems ON tblReceivDet.ReceivDetItemID = tblItems.ItemID LEFT OUTER JOIN
                         Vendor ON tblReceivMain.strVendor = Vendor.OID LEFT OUTER JOIN
                         Employees ON tblReceivMain.CheckedBy = Employees.OID LEFT OUTER JOIN
                         Employees AS Employees_1 ON tblReceivMain.UnloadedBy = Employees_1.OID LEFT OUTER JOIN
                         ShipmentConditions ON tblReceivMain.LoadCondition = ShipmentConditions.OID LEFT OUTER JOIN
                         ShipmentConditions AS ShipmentConditions_1 ON tblReceivMain.PhysicalCondition = ShipmentConditions_1.OID
